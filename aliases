help man
o _easy_open
l. ls -ld .*

# Safe remove
rmm /bin/rm
rm ~/.scripts/safe-rm.sh

# Basic Git
ga git add --verbose
gc git commit -v -S
gc! git commit -v -S --amend
gca git commit -v -S -a
gca! git commit -v -S -a --amend
gaa git add --all --verbose
gl git pull
gp git push
gpd git push --dry-run
gst git status
gd git diff
gf git fetch
gr git reset
grh git reset HEAD
grhh git reset HEAD --hard

# Branching Git
gb git branch
gco git checkout
gcb git checkout -b
gcm git checkout master
gm git merge
gmum git merge upstream/master

# Git stash
gsta git stash save
gstaa git stash apply
gstall git stash --all
gstc git stash clear
gstd git stash drop
gstl git stash list
gstp git stash pop
gsts git stash show --text
