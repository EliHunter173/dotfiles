help man
o _easy_open

# Override shell defaults
time /usr/bin/time # /usr/bin/time is standard across shells and more feature rich

# New/Better Defaults
# Pretty grep
grepp grep --color=auto --exclude-dir={.bzr,CVS,.git,.hg,.svn} --line-number --with-filename --context=1
l. ls -ld .*

# Safe remove
rmm /bin/rm
rm safe-rm

# Basic Git
ga git add --verbose
gaa git add --all --verbose
gc git commit -v
gc! git commit -v --amend
gcl git clone --recurse-submodules
gl git pull
gp git push
gpd git push --dry-run
gst git status
grm git rm
glg git lg
gd git diff
gf git fetch
grhh git reset HEAD --hard

# Git remotes
gr git remote

# Branching Git
gb git branch
gbd git branch -d
gco git checkout
gcb git checkout -b
gcm git checkout master
gm git merge

# Git stash
gs git stash save
gsp git stash pop

# Git config patcher
gitconfig_patch $SRC/github.com/elihunter173/gitconfig-patcher/gitconfig-patcher.sh ~/.dotfiles/git/gitconfigs/ncsu
